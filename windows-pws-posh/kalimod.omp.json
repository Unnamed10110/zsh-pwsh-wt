{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {

          "properties": { "display_host": true },
          "style": "plain",
          "template": "<white>\u250c\u2500\u2500(</white><green>{{ .UserName }}</green>ðŸ’€<white><#ffffff>\u2500â–¶ </#ffffff><#ff0000>{{ .HostName }}</#ff0000><#ffffff><white>)</white></ffffff>",
          "type": "session"
        },
        {
          "foreground": "yellow",
          "properties": { "fetch_version": false, "fetch_virtual_env": true },
          "style": "plain",
          "template": "<#2fff00>[\ue235</#2fff00> {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ .Full }}{{ end }}<{{ if .Root }}white{{ else }}green{{ end }}>]</>",
          "type": "python"
        },

        {
          "properties": {
            "folder_separator_icon": "/",
            "style": "full"
          },
          "style": "plain",
          "template": "<white>â”€â”€[</white><green>{{ .Path }}</green>",
          "type": "path"
        },
        {
          "type": "text",
          "template": "<white>]</white>"
        },
        {
          "type": "text",

          "template": "<#ffffff><d></d></#ffffff>"
        }
      ],
      "type": "prompt"
    },

    
    {
      "alignment": "right",
      "segments": [
        {
          "type": "git",
          "style": "plain",
          "properties": {
            "always_enabled": true,
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_gone_icon": "",
            "branch_icon": "",
            "branch_identical_icon": "",
            "cherry_pick_icon": "",
            "commit_icon": "",
            "fetch_status": true,
            "merge_icon": "",
            "rebase_icon": "",
            "revert_icon": "",
            "tag_icon": ""
          },
          "template": "<#2fff00>onâ”€â–º</#2fff00><#ffffff> {{ .HEAD }}â”€â–º</#FF0000> {{ .Staging.String }}{{ if .Working.Changed }}<#FF0000> \u25cfâ”€â–º</#FF0000> <green>{{ .Working.String }}{{ end }}</green>\n"
        }
        
      ],
      "type": "prompt"

    },
    

    {
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "template": "<white>\u2502</white> "
        }
        
      ],
      "type": "prompt"

    },



    {
      "alignment": "right",
      "newline": false,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "template": "<#ff0000>[</#ff0000>"
        },
        
        {
          "foreground":"#ffffff",
          "type": "time",
          "style": "plain",
          "properties": {
            "time_format": "<white>Monday, 02-Jan-06 15:04:05</white>"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "template": "<#ff0000>]</#ff0000>"
        }
      ],
      "type": "prompt"
    }



    ,
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "style": "plain",
          "template": "<#ffffff>\u2514\u2500â–¶<r> Î»:</r> </#ffffff>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
