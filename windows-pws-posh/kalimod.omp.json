{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "red",
          "foreground_templates": ["blue"],
          "properties": { "display_host": true },
          "style": "plain",
          "template": "<white>\u250c\u2500\u2500(</white><green>{{ .UserName }}</green>ðŸ’€<red>{{ .HostName }}</red><white>)</white>",
          "type": "session"
        },
        {
          "foreground": "yellow",
          "properties": { "fetch_version": false, "fetch_virtual_env": true },
          "style": "plain",
          "template": "<{{ if .Root }}white{{ else }}green{{ end }}>-[</>\ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }}{{ end }}{{ .Full }}{{ end }}<{{ if .Root }}white{{ else }}green{{ end }}>]</>",
          "type": "python"
        },

        {
          "foreground": "green",
          "properties": {
            "folder_separator_icon": "<#c0c0c0>/</>",
            "style": "full"
          },
          "style": "plain",
          "template": "<white>â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€[</white>{{ .Path }}]</>",
          "type": "path"
        },
        {
          "type": "text",
          
          "template": "<white><d>'...I can always do it tomorrow...'</d></white>"
        }
      ],
      "type": "prompt"
    },

    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "white",
          "properties": { "always_enabled": true, "style": "round" },
          "style": "plain",
          "template": " {{ .FormattedMs }} ",
          "type": "executiontime"
        },
        {
          "foreground": "green",
          "foreground_templates": ["{{ if gt .Code 0 }}red{{ end }}"],
          "properties": { "always_enabled": true },
          "style": "plain",
          "template": " {{ if gt .Code 0 }}\uea76{{else}}\uf42e{{ end }} ",
          "type": "status"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "white",
          "style": "plain",
          "template": "<white>\u2514\u2500</white><green> Î»: </green>",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 3
}
