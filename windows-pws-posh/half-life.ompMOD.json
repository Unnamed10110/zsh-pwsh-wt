{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 3,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        
        {
          "type": "text",
          "style": "plain",
          "foreground": "#87FF00",
          "template": "───────────────────────────────────── ... I can always do it tomorrow ... ─────────────────────────────────────"
        },

        {
          "type": "session",
          "style": "plain",
          "foreground": "#ff0000",
          "template": "\n<b>{{ .UserName }}</b>  >>  "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#87FF00",
          "template": "in "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#88ff00",
          "properties": {
            "style": "full"
          },
          "template": "{{ .Path }} "
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#5FD7FF",
          "properties": {
            "branch_ahead_icon": "",
            "branch_behind_icon": "",
            "branch_gone_icon": "",
            "branch_icon": "",
            "branch_identical_icon": "",
            "cherry_pick_icon": "",
            "commit_icon": "",
            "fetch_status": true,
            "merge_icon": "",
            "rebase_icon": "",
            "revert_icon": "",
            "tag_icon": ""
          },
          "template": "<#ffffff>on</> {{ .HEAD }}{{ if .Staging.Changed }}<#FF0000> \u25cf {{ .Staging.String }}</>{{ end }}{{ if .Working.Changed }}<#FF0000> \u25cf {{ .Working.String }}</>{{ end }} "
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#0088ff",
          "template": "["
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#ffffff",
          "properties": {
            "time_format": "Monday, 02-Jan-06 15:04:05 MST"
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#0088ff",
          "template": "]\n"
        },


        {
          "type": "text",
          "style": "plain",
          "foreground": "#0088ff",
          "template": "["
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "unnamed10110"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#0088ff",
          "template": "]"
        },

        {
          "type": "text",
          "style": "plain",
          "foreground": "#88ff00",
          "template": " >> λ : "
        }
      ]
    }
  ]
}
